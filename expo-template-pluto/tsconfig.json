{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@common/*": ["./src/common/*"],
      "@components/*": ["./src/components/*"],
      "@modules/*": ["./src/modules/*"],
      "@assets/*": ["./assets/*"],
      "@configs/*": ["./src/configs/*"],
      "@lib/*": ["./src/lib/*"]
    }
  }
}
